  "scripts": {
    "jss": "jss",
    "start": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components",
    "start:env": "scjss-verify-setup && npm-run-all --serial bootstrap:env start:react start:watch-components",
    "build": "npm-run-all --serial bootstrap:connected build:client build:server",
    "build:env": "npm-run-all --serial bootstrap:env build:client build:server",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "node ./scripts/start.js",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"node ./scripts/build.js\"",
    "build:server": "cross-env-shell NODE_ENV=production \"webpack --config ./server/server.webpack.config.js\"",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:env": "node scripts/bootstrap.js --env",
    "graphql:update": "node -r babel-register ./scripts/update-graphql-fragment-data.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "SET NODE_ENV=development&&node .\\node_modules\\eslint\\bin\\eslint.js ./**/client/**/*.js ./**/client/**/*.jsx -c .eslintrc --ignore-pattern node_modules",
    "lint:fix": "npm run lint -- --fix",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie < 11",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^foundation(.*)$": "<rootDir>/Foundation$1",
      "^feature(.*)$": "<rootDir>/Feature$1",
      "^assets(.*)$": "<rootDir>/Project/Certification/client/assets$1",
      "^project(.*)$": "<rootDir>/Project$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./Project/Certification/client"
          ],
          "alias": {
            "foundation": "./Foundation",
            "feature": "./Feature",
            "assets": "./Project/Certification/client/assets",
            "project": "./Project"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
